{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": { "*": ["src/types/*"] },
      "outDir": "./dist/",
      "target": "es5",
      "sourceMap": true,
      "jsx": "react",
      "rootDir": "./",
      "module": "commonjs",
      "lib": [
        "es2018",
        "dom"
      ],
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true
    },
  }